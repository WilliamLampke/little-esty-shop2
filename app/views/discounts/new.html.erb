<%= form_with url: "/merchant/#{@merchant.id}/discounts", method: :post, local: true do |f| %>
  <%= f.label :percentage, "Percentage (float)" %>
  <%= f.text_field :percentage %>
  <%= f.label :quantity_threshold, "Quantity Threshold" %>
  <%= f.text_field :quantity_threshold %>
  <%= f.submit "Submit" %>
<% end %>